{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": ".",
    "paths": {
      "@/common/*": [
        "../src/common/*"
      ],
      "@/components/*": [
        "../src/common/components/*"
      ],
      "@/utils/*": [
        "../src/common/utils/*"
      ],
      "@/modules/*": [
        "../src/modules/*"
      ]
    },
    "lib": ["es5", "dom"],
    "types": ["cypress", "node", "cypress-visual-regression",  "cypress-real-events"]
  },
  "include": ["**/*.ts"]
}